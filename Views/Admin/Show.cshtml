@model List<TermProject.Models.UserTable>
@{
    ViewBag.Title = "Show";
}

<h2>All Users</h2>
@Html.ActionLink("Admin Menu", "Index") Click Here to go to Admin Menu.
<table class="table table-striped">
   <tr>
       <td>Primary Key</td>
       <td>Name</td>
       <td>Username</td>
       <td>Email</td>
       <td>Password</td>
       <td>Update</td>
       <td>Delete</td>
   </tr>
    @foreach (var a in Model)
    { 
        <tr>
            <td>@a.Id</td>
           <td>@a.Name</td>
           <td>@a.Username</td>
           <td>@a.Email</td>
           <td>@a.Password</td>
           <td><a href="/Admin/Update/@a.Id">Update</a></td>
           <td><a href="/Admin/Delete/@a.Id">Delete</a></td>
        </tr>
    }
</table>

